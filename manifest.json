{
    "manifest_version": 3,
    "name": "Code Executor Extension",
    "version": "1.0",
    "description": "Opens a WebSocket to localhost and sends code snippets from GitHub pages.",
    "permissions": ["scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.github.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_icon": "icons/icon.svg"
    }
  }
  